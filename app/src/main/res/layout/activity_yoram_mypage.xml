<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/yoramMyPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".YoramMypage"
    android:orientation="vertical">
    <TextView
        android:id="@+id/yoram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="79dp"
        android:text="@string/mypage_title"
        android:textAppearance="@style/yoram"
        android:textColor="@color/black"
        android:lineSpacingExtra="-6sp"
        android:layout_gravity="center"
        />
    <LinearLayout
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/continuous_day"
            android:text="12"
            android:layout_marginRight="5dp"
            android:textSize="35dp"
            android:textAppearance="@style/explain_text"
            android:textColor="#FF46C5"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="일 연속으로 요가를 수행 했어요!"
            android:textSize="20dp"
            android:textAppearance="@style/explain_text"/>
    </LinearLayout>

    <CalendarView

        android:layout_marginTop="70dp"
        android:id="@+id/calendarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />



</LinearLayout>
</FrameLayout>